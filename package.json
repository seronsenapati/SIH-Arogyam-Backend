{
  "name": "arogyam-backend",
  "version": "1.0.0",
  "description": "A complete, working backend for the Arogyam telehealth project",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --runInBand",
    "lint": "eslint .",
    "format": "prettier --write .",
    "seed": "node src/scripts/seed.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seronsenapati/SIH-Arogyam-Frontend.git"
  },
  "keywords": [
    "telehealth",
    "healthcare",
    "appointment",
    "nodejs",
    "express",
    "mongodb"
  ],
  "author": "Arogyam Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seronsenapati/SIH-Arogyam-Frontend/issues"
  },
  "homepage": "https://github.com/seronsenapati/SIH-Arogyam-Frontend#readme",
  "dependencies": {
    "aws-sdk": "^2.1692.0",
    "axios": "^1.12.0",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongoose": "^8.18.1",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.6",
    "sendgrid": "^5.2.3"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.5.0",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.4"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/tests"
    ],
    "testMatch": [
      "**/*.test.js"
    ],
    "verbose": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/tests/**/*.js",
      "!src/server.js"
    ]
  }
}
